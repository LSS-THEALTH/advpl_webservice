#Include "TOTVS.CH"
#INCLUDE "TOTVSWEBSRV.CH"


WSSERVICE EXERC1 Description "EXERCICIO 1 DE AULA"

	WSDATA cDados   AS STRING
	WSDATA cData	AS STRING

	WSMETHOD MOSTRADATA Description "DATA ATUAL"
	WSMETHOD MOSTRAHORA Description "HORA ATUAL"

ENDWSSERVICE


WSMETHOD MOSTRADATA WSRECEIVE NULLPARAM WSSEND cData WSSERVICE EXERC1
::cData := DTOC(DATE())
RETURN .T. 


WSMETHOD MOSTRAHORA WSRECEIVE cDados WSSEND cData WSSERVICE EXERC1
::cData := TIME()
RETURN .T. 
